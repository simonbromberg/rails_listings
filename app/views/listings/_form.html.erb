
<%= bootstrap_form_for(@listing) do |f| %>
	<%= f.text_field :name %>
	<%= f.text_field :description %>
	<%= f.text_field :address %>
	<%= f.text_field :city %>
	<%= f.text_field :state %>
	<%= f.text_field :country %>
	<%= f.text_field :postal_code %>
	<%= f.text_field :website %>
	<%= f.text_field :logo_url %>
	<%= f.submit "Create" %>
<% end %>